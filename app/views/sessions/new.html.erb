<h1 class="header">Log In</h1>
<%= form_tag sessions_path do %>
<div class="form">
  <div>
    <%= label_tag :email %><br/>
    <%= text_field_tag :email %>
  </div>
  <div>
    <%= label_tag :password %><br/>
    <%= password_field_tag :password %>
  </div>
  <div><%= submit_tag "Log In" %></div>
</div>
<% end %>